# PostgreSQL Secrets
POSTGRES_USER=n8n
POSTGRES_PASSWORD=GEN_ALNUM_32
POSTGRES_DB=n8n

# n8n Database Password (must match POSTGRES_PASSWORD)
DB_POSTGRESDB_PASSWORD=${POSTGRES_PASSWORD}

# n8n Encryption Key (CRITICAL - do not lose)
N8N_ENCRYPTION_KEY=GEN_HEX_64

# PostgreSQL Connection String (for monitoring)
# Must match POSTGRES_PASSWORD above
POSTGRES_CONNECTION=postgresql://n8n:${POSTGRES_PASSWORD}@postgres:5432/n8n?sslmode=disable
